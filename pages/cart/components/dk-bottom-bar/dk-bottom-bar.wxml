<!--pages/cart/components/dk-bottom-bar/dk-bottom-bar.wxml-->
<view class="bar">
  <view class="status">
     <checkbox-group bindchange="allCheck">
      <label>
          <checkbox color="#ff5777" checked="{{total.allChecked ? 'checked' : ''}}"></checkbox>全选
      </label>
     </checkbox-group>
  </view>
  <view class="total">
    总计: ￥{{total.total}}
  </view>
  <view class="pay">
    付款({{total.count}})
  </view>
</view>


